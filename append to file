using namespace std;

int main() {
	
	fstream file;
	
	const char* filePath = "files/myTxt3.txt";
	
	file.open(filePath, ios::app  ); // output mode (writing on the file mode)
	
	if (file.is_open()) {
		file << "hello my name is saif \n";
		file << "im using python to program.\n";
	}
	
	file.close();
	
	

}
