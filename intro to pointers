#include "MyLib.h"
#include <array>
#include <vector>
#include <iostream>
#include <iomanip>
#include <string>
using namespace std;



int main() {

	int a = 1;
	cout << "the value of a : " << a<< endl;
	cout << "the address of a : " << &a << endl;

	int* p = &a;
	cout << "the value of p : " << p << endl;
	cout << "the address of p : " << &p << endl;

	int b = 30;

	cout << "the value of b : " << b << endl;
	cout << "the address of b : " << &b << endl;

	p = &b;

	cout << "the value of p : " << p << endl;
	cout << "the address of p : " << &p;


}
